<html lang="en">
    <head>
        <title>Users App</title>
    </head>
    <body>
        <div>
            <h3>Users App</h3>
            <ul>
                <li class="active"><a href="/users">List All Users </a></li>
                <li><a href="/users/delete">Delete a User</a></li>
                <li><a href="/users/create">Create a User</a></li>
            </ul>
        </div>
        <div>
            <h2>List of All Users</h2>
            <table>
                <thead>
                    <tr>
                        <th>User ID  </th>
                        <th>First Name  </th>
                        <th>Last Name  </th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(function(user){ %>
                        <tr>
                            <td><%= user.UserID %>  </td>
                            <td><%= user.FirstName %>  </td>
                            <td><%= user.LastName %>  </td>
                        </tr>
                        
                    <%  }); %>
                </tbody>
            </table>
        </div>              
    </body>
</html>